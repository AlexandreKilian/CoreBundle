{% if ngAdmin %}
<block name="{{block.name}}" page="{{block.page.id}}" blockid="{{block.id}}"></block>
{% else %}
{% for element in children %}
{% if element.elementType == 'widget' %}
  {{ render(controller('BrixCoreBundle:Widget:renderWidget', {
    widget: element
  })) }}
{% else %}
  {{ render(controller('BrixCoreBundle:Block:renderBlockElement', {
    block: element
  })) }}
  {% endif %}
{% endfor %}
{% endif %}
